@article{WendrichW.2010Dsss,
	Abstract = {The earliest evidence of the use of domesticated plants, a traditional hallmark of Neolithic societies in the ancient Near East, first appears in Egypt in archaeological sites in the Fayum depression. Due to wind erosion often resulting in deflation of sediments in this region, stratified sites containing organic materials are rare and the depositional contexts of some earlier 14C measurements on Fayum Neolithic materials are not precisely documented. We report the results of 29 AMS-based 14C determinations on charcoal recovered from stratified contexts in two Fayum Neolithic village sites, Kom K and Kom W. These data assign a mid-5th millennium BCE age to these sites and permit an estimate of the length of their occupation to be approximately three centuries.},
	Author = {Wendrich, W. and Taylor, R.E. and Southon, J.},
	Issn = {0168-583X},
	Journal = {Nuclear Inst. and Methods in Physics Research, B},
	Keywords = {Egyptian Archaeology ; Neolithic ; Fayum Neolithic ; Birket Qarun},
	Language = {eng},
	Number = {7},
	Pages = {999--1002},
	Publisher = {Elsevier B.V.},
	Title = {Dating stratified settlement sites at Kom K and Kom W: Fifth millennium BCE radiocarbon ages for the Fayum Neolithic},
	Volume = {268},
	Year = {2010}}

@article{WenglerVernet.1994EvChr,
	Abstract = {Researches on prehistoric sites and recent quaternary formations of eastern Morocco point out the existence of three inset glacis-terrace systems, slope and sheltered deposits which are attributed to the Holocene or Rharbian of the Moroccan chronology. Through their geomorphological, stratigraphical, sedimentological, chronological and paleontological study in the Wadi El Hay and Wadi Isly basins, the regional history of geological events, vegetation and fauna during this period could be recalled and the important part of man in the genesis of archaeological sedimentary deposits emphasized. Precisions have been brought on palaeoenvironments and particularly on climatic variations which may distinguish many and or humid phases in Holocene. The results present many convergences with the data coming from different regions of Maghreb. On this basis, they may working out a chronology of the Rharbian.},
