@article{MerzougKherbouche.2016Fauna,
	Abstract = {The GDL1 cave belongs to the large karst network of the Adrar Gueldaman ridges (Northern Algeria). New excavations by the Centre National de Recherches Pr{\'e}historiques, Anthropologiques et Historiques (CNRPAH) since 2010 have yielded rich archaeological remains dated between 7002 cal BP and 1484 cal BP. The paper presents results of the zooarchaeological analysis of the macromammals from the Neolithic units in sectors S2 and S3. They reveal the presence of a restricted fauna, dominated by sheep and goat, goat being more frequent than sheep. Domestic cattle and pig may also be present. The age profiles of the goat/sheep finds suggest a shift in the management of the sheep/goat herds aiming first at the production of meat and later at the production of meat and secondary products.},
	Author = {Merzoug, Souhila and Kherbouche, Farid and Sehil, Naanaa and Chelli, Razika and Hachi, Slimane},
